{
  "name": "miniAppTest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start:mini": "react-native start --port 9000",
    "test": "jest",
    "pod": "cd ios && RCT_NEW_ARCH_ENABLED=0 pod install && cd ..",
